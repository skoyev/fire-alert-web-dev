{
  "name": "angular-io-example",
  "version": "1.0.0",
  "private": true,
  "description": "Example project from an angular.io guide.",
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod",
    "start": "ng serve",
    "start_new": "npm run env -s && ng serve --aot",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "e2e": "ng e2e"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/platform-server": "~7.0.0",
    "@angular/router": "~7.0.0",
    "@angular/upgrade": "~7.0.0",
    "@ngx-translate/core": "^10.0.1",
    "@ngrx/effects": "^6.1.0",
    "@ngrx/entity": "^6.1.0",
    "@ngrx/router-store": "^6.1.0",
    "@ngrx/store": "^6.1.0",
    "@ngrx/store-devtools": "^6.1.0",
    "angular-in-memory-web-api": "~0.6.1",
    "core-js": "^2.5.7",
    "ngrx-store-freeze": "^0.2.4",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26",
    "@ng-bootstrap/ng-bootstrap": "^3.3.0",
    "bootstrap": "^4.1.1",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "webpack": "~3.11.0",
    "webpack-dev-middleware": "~1.12.0",
    "webpack-dev-server": "~2.11.0",
    "webpack-merge": "^4.1.0",
    "webpack-sources": "^1.0.0",
    "webpack-subresource-integrity": "^1.0.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.5",
    "@angular/cli": "~7.0.2",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/language-service": "^7.0.0",
    "@ngrx/schematics": "^6.1.0",
    "@types/jest": "^23.3.5",
    "@types/node": "^10.11.7",
    "codelyzer": "~4.5.0",
    "jasmine-marbles": "^0.4.0",
    "jest": "^23.6.0",
    "jest-preset-angular": "^6.0.1",
    "lodash": "^4.17.11",
    "phantomjs-prebuilt": "^2.1.16",
    "protractor": "~5.4.1",
    "rxjs-compat": "^6.3.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "~3.1.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "coveragePathIgnorePatterns": [
      "/jest-config/",
      "/node_modules/"
    ],
    "setupTestFrameworkScriptFile": "./jest-config/setup.ts",
    "moduleNameMapper": {
      "@appContainers/(.*)": "<rootDir>/src/app/containers/$1",
      "@appComponents/(.*)": "<rootDir>/src/app/components/$1",
      "@appServices/(.*)": "<rootDir>/src/app/services/$1",
      "@appStore/(.*)": "<rootDir>/src/app/store/$1",
      "@appGuards/(.*)": "<rootDir>/src/app/guards/$1",
      "@appModels/(.*)": "<rootDir>/src/app/models/$1",
      "@appShared/(.*)": "<rootDir>/src/app/shared/$1"
    }
  },
  "repository": {}
}
